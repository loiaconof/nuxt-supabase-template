    "@nuxtjs/supabase": "^1.2.2",
    "@prisma/client": "^5.14.0",
    "nuxt": "^3.11.2",
    "prisma": "^5.14.0",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.20.0",
    "eslint": "^9.4.0"
  }
}
